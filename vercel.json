{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "build/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "build/$1"
    },
    {
      "src": "/add-team",
      "dest": "index.js"
    },
    {
      "src": "/add-team/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/process-result",
      "dest": "index.js"
    },
    {
      "src": "/process-result/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/team-result",
      "dest": "index.js"
    },
    {
      "src": "/team-result/(.*)",
      "dest": "index.js"
    }
  ]
}
